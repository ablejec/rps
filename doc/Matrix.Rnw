\section{Test hipotez}

%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Test hipotez}
Ali so fantje večji od deklet
<<velikost>>=
t.test(visina~spol,data=data)
@
\end{frame}
%% <<<<<<---------------------|


%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Funkcija za Student t-test}
Ali so fantje večji od deklet
<<ttest>>=
student <- function(x,y,delta=0){
# funkcija za dva vzorca
n1 <- length(x)
n2 <- length(y)
m1 <- mean(x)
m2 <- mean(y)
s1 <- sd(x)
s2 <- sd(y)
####
ss <- ((n1-1)*s1^2+(n2-1)*s2^2)/(n1+n2-2)
t <- (m1-m2-delta)/sqrt(ss)*sqrt(n1*n2/(n1+n2))
# debug
#print(t)
p <- 2 * (1- pt(abs(t),n1+n2-2)  )
cat("\tStudent t-test\n\np =",p,"\n")
invisible(p)
}
@
\end{frame}
%% <<<<<<---------------------|

%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Preizkus}
<<>>=
X1 <- rnorm(20)
X2 <- rnorm(10,5)
student(X1,X2)

@
\end{frame}
%% <<<<<<---------------------|

%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Visina}
<<>>=
X1 <- visina[spol=="M"]
X2 <- visina[spol=="F"]
student(X1,X2)


@
\end{frame}
%% <<<<<<---------------------|



