diag(diag(S))
SD1<-sqrt(diag(1/diag(S)))
SD1

@
\end{frame}
%% <<<<<<---------------------|



%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Korelacijska matrika}
<<>>=
# korelacijska matrika
R<-SD1%*%S%*%SD1
R

@
\end{frame}
%% <<<<<<---------------------|



%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Še enkrat}
<<>>=
