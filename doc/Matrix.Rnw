@
\end{frame}
%% <<<<<<---------------------|

<<>>=
t(X)
X
@



%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Matrika SSP}
<<SSP>>=
# SSP
C<-t(X)%*%X
C

@
\end{frame}
%% <<<<<<---------------------|



%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Kovariančna matrika}
<<>>=
# kovariančna matrika
S<-C/(n-1)
S
cov(X)
diag(S)

@
\end{frame}
%% <<<<<<---------------------|



%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Matrika varianc}
<<>>=
